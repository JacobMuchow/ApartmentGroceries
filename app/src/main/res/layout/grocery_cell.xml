<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <!-- grocer name -->
    <TextView
        android:id="@+id/grocery_cell_grocery_name_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/group_cell_name_id"
        android:layout_margin="5dp"
        android:text="grocery name"/>

    <!-- created by -->
    <TextView
        android:id="@+id/grocery_cell_created_by_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/grocery_cell_grocery_name_id"
        android:layout_margin="5dp"
        android:text="createdBy"/>

</RelativeLayout>